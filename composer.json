{
    "name": "tkrebs/ep3-bs",
    "description": "Online booking system for courts",
    "license": "MIT",
    "homepage": "http://bs.hbsys.de/",

    "require": {
        "php": ">=5.6 <=8.4",
        "ext-intl": "*",
        "zendframework/zendframework": "^2.5.3",
        "symfony/polyfill-php71": "^1.19",    
        "payum/payum-module": "*@stable",
        "payum/payum": "*@stable",
        "php-http/guzzle6-adapter": "^2.0",
        "payum/paypal-express-checkout-nvp": "^1.5",
        "stripe/stripe-php": "6.9.0",
        "payum/stripe": "^1.6",
        "payum/klarna-checkout": "^1.5",
        "eluceo/ical": "^0.16.0",
        "php-http/message-factory": "^1.1",
        "klarna/checkout": "^4.0"
    },

    "autoload": {
        "psr-4": {
            "Zend\\I18n\\": "module/Zend/I18n/src/",
            "Zend\\Stdlib\\": "module/Zend/Stdlib/src/",
            "Zend\\View\\": "module/Zend/View/src/"
        }
    },    
    
    "repositories": [
    {
        "type": "git",
        "url": "https://github.com/lolmx/Stripe.git"
